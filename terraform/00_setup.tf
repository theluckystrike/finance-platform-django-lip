terraform {
  cloud {

    organization = "oland-investments"

    workspaces {
      name = "oland-investments-prod"
    }
  }
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 4.16"
    }
  }

  required_version = ">= 1.2.0"
}

provider "aws" {
  region = var.region
}